<% include header %>

<div class="app">
  <div class="appHeader">
    <div class="appName">
      CryptoHocrux
    </div>
    <div class="appSubtext">
      "Thou shall never be lost again"
    </div>
  </div>
  <div class="appNav">
    <a href="/">
      <div class="appNavEntry">
        Create Hocrux
      </div>
    </a>
    <a href="/combine">
      <div class="appNavEntry">
        Combine Hocrux
      </div>
    </a>
    <div class="clear"></div>
  </div>
  <div class="appSection">
    <div>
      <img class="icon icons8-Safe" src="data:image/png;base64," width="48" height="48">
    </div>
    <div class="appSectionTitle">
      Horcrux Input
    </div>
    <div class="appInstruction">
      Enter your hocrux to retrieve your private key
    </div>
    <div class="inputArea">
      <div>
        <input type="text" id="combineHocrux1">
      </div>
      <div>
        <input type="text" id="combineHocrux2">
      </div>
      <div>
        <input type="text" id="combineHocrux3">
      </div>
      <div>
        <input type="text" id="combineHocrux4">
      </div>
      <div>
        <input type="text" id="combineHocrux5">
      </div>
      <div>
        <input type="text" id="combineHocrux6">
      </div>
      <div>
        <input type="text" id="combineHocrux7">
      </div>
      <div>
        <input type="text" id="combineHocrux8">
      </div>
      <div>
        <input type="text" id="combineHocrux9">
      </div>
      <div>
        <input type="text" id="combineHocrux10">
      </div>
    </div>
    <button id="submit">Combine Hocrux</button>
  </div>
  
  <div class="appSection">
    <div>
      Password: <input type="password">
    </div>
    <div>
      Salt: <input type="text">
    </div>
  </div>
  
  
  <div class="appSection">
    <div>
      <img class="icon icons8-Refund" src="data:image/png;base64," width="48" height="48">
    </div>
    <div class="outputArea">
      
    </div>
  </div>
</div>

<script>
  $(function() {
    
    $("#submit").click(function(){
        $.ajax({
          url: '/combine-hocrux',
          type: 'POST',
          data: {
            combineHocrux1: $("#combineHocrux1").val(),
            combineHocrux2: $("#combineHocrux2").val(),
            combineHocrux3: $("#combineHocrux3").val(),
            combineHocrux4: $("#combineHocrux4").val(),
            combineHocrux5: $("#combineHocrux5").val(),
            combineHocrux6: $("#combineHocrux6").val(),
            combineHocrux7: $("#combineHocrux7").val(),
            combineHocrux8: $("#combineHocrux8").val(),
            combineHocrux9: $("#combineHocrux9").val(),
            combineHocrux10: $("#combineHocrux10").val(),
          },
          success: function(data){
            $(".outputArea").html("");
            $(".outputArea").append('<div>'+data+'</div>');
          },
          error: function(data){
            
          }
        });
    })
  });
</script>

<% include footer %>